using System;

class Program
{
    static void Main()
    {
        Console.Write("Enter number of terms: ");
        int n = Convert.ToInt32(Console.ReadLine());
        int a = 0, b = 1, c;

        Console.WriteLine("Fibonacci Series:");
        Console.WriteLine(a);
        Console.WriteLine(b);

        for (int i = 2; i < n; i++)
        {
            c = a + b;
            Console.WriteLine(c);
            a = b;
            b = c;
        }
    }
}
